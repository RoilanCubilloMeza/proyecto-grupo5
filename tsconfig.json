{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": false,
    "paths": {
      "<nuevo>/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.js", "**/*.ts", "**/*.tsx", "src/pages/signuptsx", "src/pages/signuptsx", "next.config.js", "tailwind.config.js"],
  "exclude": ["node_modules"]
}
