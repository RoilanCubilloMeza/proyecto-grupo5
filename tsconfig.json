{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": false,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.js", "**/*.ts", "**/*.tsx", "src/pages/signuptsx", "src/pages/signuptsx", "next.config.js", "tailwind.config.js", "src/pages/prueba.js", "src/pages/index.jsx", "src/pages/user.jsx", "src/pages/user.jsx", "src/redux/store.js", "src/redux/reducer/userReducer.js", "src/redux/actions/user.Actions.js", "src/redux/actions/user.Actions.js"],
  "exclude": ["node_modules"]
}
